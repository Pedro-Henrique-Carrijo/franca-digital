def trocar_valores(a, b):
    # Troca os valores de a e b usando a atribuição múltipla
    a, b = b, a
    return a, b

# Testando a função
x = int(input("Digite o valor de x: "))
y = int(input("Digite o valor de y: "))

# Mostra os valores antes da troca
print(f"Antes da troca: x = {x}, y = {y}")

# Troca os valores
x, y = trocar_valores(x, y)

# Mostra os valores após a troca
print(f"Após a troca: x = {x}, y = {y}")
