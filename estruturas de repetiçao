numero = 5
contador = 1

while contador <= 10:
    resultado = numero * contador
    print(f"{numero} x {contador} = {resultado}")
    contador += 1

    numero = int(input("Digite um nÃºmero para ver sua tabuada: "))

for contador in range(1, 11):
    resultado = numero * contador
    print(f"{numero} x {contador} = {resultado}")
